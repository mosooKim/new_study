   "source": [
    "# BOJ - 2480 주사위 세개\n",
    "\n",
    "dices = sorted(list(map(int, input().split())))\n",
    "\n",
    "def solution(dices):\n",
    "    if len(set(dices)) == 1:\n",
    "        return 10000 + dices[0] * 1000\n",
    "    elif len(set(dices)) == 2:\n",
    "        return 1000 + dices[1] * 100\n",
    "    else:\n",
    "        return max(dices) * 100\n",
    "\n",
    "solution(dices)"
   ]
  }
